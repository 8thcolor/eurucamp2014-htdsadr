.readme-holder#README
  %h4
    %i.icon-file
      = readme.name
  .wiki
    - if gitlab_markdown?(readme.name)
      = preserve do
        = markdown(readme.data)
    - elsif plain_text_readme?(readme.name)
      %pre.clean
        = readme.data
    - elsif markup?(readme.name)
      = render_markup(readme.name, readme.data)
